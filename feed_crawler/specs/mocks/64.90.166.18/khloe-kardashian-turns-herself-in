<script type="text/javascript">	
			$(document).ready(
			    function() {	
				
				var today = new Date();
				var first = new Date(today.getFullYear(), 0, 1);
				var theDay = Math.round(((today - first) / 1000 / 60 / 60 / 24) + .5, 0);
			
				if (theDay % 2 == 0) {
					// Show the Fox feed if it's an even day.
					$("#fox_feed").css({ display:"block" });
					$(".show_fox_news_box").css({ background: "#dddddd", color: "#e30f71" });
				} else {
					// Show the Just Jared feed if it's an odd day.
					$("#just_jared_feed").css({ display:"block" });
					$(".show_just_jared_box").css({ background: "#dddddd", color: "#e30f71" });
				}
				
						$('.show_fox_news').click(
						function() { 
							$("#just_jared_feed").css({ display:"none" });
							$("#fox_feed").css({ display:"block" });
							$(".show_fox_news_box").css({ background: "#dddddd", color: "#e30f71" });
							$(".show_just_jared_box").css({ background: "#ffffff", color: "#000000" });

						});
						
						$('.show_just_jared').click(
						function() { 
							$("#fox_feed").css({ display:"none" });
							$("#just_jared_feed").css({ display:"block" });
							$(".show_just_jared_box").css({ background: "#dddddd", color: "#e30f71" });
							$(".show_fox_news_box").css({ background: "#ffffff", color: "#000000" });

						});
				
				
				});
</script>		
								

