<script type="text/javascript">
		function showRTab(){
			var r = Math.round(100*Math.random());
			if( r < 50 ){
				updatePeople();
				$('people_tab').addClassName('selected');
				$('instyle_tab').removeClassName('selected');
			} else {
				updateInstyle();
				$('instyle_tab').addClassName('selected');
				$('people_tab').removeClassName('selected');
			}
		}
		
		
		function toggleTabs() {
			$('people_tab').toggleClassName('selected');
			$('instyle_tab').toggleClassName('selected');
		};
		var instyle_logo_url = "instyle.gif";		
		var xid = "tcm_pop";		
		function updateInstyle() {
			var instyle_widget = new Template('<div id="widget_title"><a class="nohover" target="new" href="http://www.instyle.com/instyle/celebrities/lookoftheday/photos/0,,,00.html?xid=' + xid + '"><img src="http://images.teamsugar.com/static/imgs/' + instyle_logo_url + '" style="border: none" /></a></div><br style="clear: both;" /><img src="#{image}" style="float: left; margin-right: 1em; margin-bottom: 0.2em; border: 1px solid #666; margin-top: 1px;" />#{date}<br /><br />#{body}');			
			$('people_instyle_widget').update(instyle_widget.evaluate({
				date: instyle.lookoftheday.articledate,
				image: instyle.lookoftheday.image,
				body: instyle.lookoftheday.body
			}));
			toggleTabs();
			return true;
		};
		
		function updatePeople() {
			var people_widget = new Template('<div id="widget_title"><a target="new" class="nohover" href="http://www.people.com/people?xid=popsugar"><img src="http://images.teamsugar.com/static/imgs/peoplestylewatch.gif" style="border: none;"></a></div><br style="clear: both"><div id="people_widget" style="width: 290px; overflow: hidden;"><table border="0" cellspacing="4"><tr><td valign="top" width="75px"><a href="#{url1}" target="new"><img src="#{image1}" style="height: 75px; width: 75px;"></a></td><td valign="top"><div style="width: 190px;"><a href="#{url1}" target="new">#{title1}</a><br>#{body1}</div></td></tr><tr><td colspan="2" style="height: 8px;">&nbsp;</td></tr><tr><td valign="top" width="75px"><a href="#{url2}" target="new"><img src="#{image2}" style="height: 75px; width: 75px;"></a></td><td valign="top"><div style="width: 190px;"><a href="#{url2}" target="new">#{title2}</a><br>#{body2}</div></td></tr><tr><td colspan="2" style="height: 8px;">&nbsp;</td></tr><tr><td valign="top" width="75px"><a href="#{url3}" target="new"><img src="#{image3}" style="height: 75px; width: 75px;"></a></td><td valign="top"><div style="width: 190px;"><a href="#{url3}" target="new">#{title3}</a><br>#{body3}</div></td></tr></table></div><br style="clear: left;"><center><a href="http://offtherack.people.com?xid=popsugar" target="new">More StyleWatch Photos at PEOPLE.com</a></center>');
			$('people_instyle_widget').update(people_widget.evaluate({
				body1: people.story1.body.unescapeHTML(),
				body2: people.story2.body.unescapeHTML(),
				body3: people.story3.body.unescapeHTML(),
				title1: people.story1.title.gsub('>','').unescapeHTML(),
				title2: people.story2.title.gsub('>','').unescapeHTML(),
				title3: people.story3.title.gsub('>','').unescapeHTML(),
				image1: people.story1.image,
				image2: people.story2.image,
				image3: people.story3.image,
				url1: people.story1.url,
				url2: people.story2.url,
				url3: people.story3.url
			}));
			toggleTabs();
			return true;
		};
		
		Event.observe(document.body,'load', new function(){
			showRTab();
			
			Event.observe('people_tab','click',updatePeople);
			Event.observe('instyle_tab','click',updateInstyle);
		});
		
		
</script>
